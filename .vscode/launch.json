{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Active CMake Target",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "stutil",
            "cwd": "${workspaceFolder}",
            "executable": "${command:cmake.launchTargetPath}",
            "device": "STM32F446RE",
            "interface": "swd",
            "runToEntryPoint": "main",
            "svdFile": "${workspaceFolder}/STM32F401.svd"
        }
    ]
}